<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ExtraText">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TITSPORTS"
        tools:targetApi="31">
        <activity
            android:name=".Poster"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".FullScreenImageActivity"
            android:exported="false" />
        <activity
            android:name=".GoldCupHighlights"
            android:exported="false" />
        <activity
            android:name=".NodalHighlights"
            android:exported="false" />
        <activity
            android:name=".WhatsappGroup"
            android:exported="false" />
        <activity
            android:name=".InterCollegeRegistration"
            android:exported="false" />
        <activity
            android:name=".NodalRegistration"
            android:exported="false" />
        <activity
            android:name=".UpdateProfile"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ForgetPassword"
            android:exported="false"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UploadCertificate"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MainActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>